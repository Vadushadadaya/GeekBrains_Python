S = []
summ = 0
n = 0 
n_sum = 0
for i in range(1, 1000, 2):
    S.append(i**3)
    n = i**3
    while n > 0:
        n_sum += n % 10
        n //= 10
    if n_sum % 7 == 0:
        summ += i**3
print(summ)
summ = 0 
n_summ = 0
for i in range(len(S)):
    S[i]+=17
    n = S[i]
    while n > 0:
        n_sum += n % 10
        n //= 10
    if n_sum % 7 == 0:
        summ += S[i]
print(summ)
    
            